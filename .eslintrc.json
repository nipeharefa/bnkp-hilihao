{
    "extends": "airbnb",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "rules": {
        "react/forbid-prop-types": "off",
        "react/prop-types": ["warn"],
        "no-unused-vars": "warn",
        "function-paren-newline": "off"
    },
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ],
    "settings": {
        "import/resolver": {

        }
    }
}